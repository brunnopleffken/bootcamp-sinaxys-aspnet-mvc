@model List<Publisher>

<h1>Editoras</h1>

<div class="text-end">
    <a asp-action="New" class="btn btn-primary">Adicionar editora</a>
</div>

<table class="table">
    <thead>
    <tr>
        <th>ID</th>
        <th>Nome da editora</th>
        <th>Cidade</th>
        <th>Pa√≠s</th>
    </tr>
    </thead>
    <tbody>
    @foreach (Publisher item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.City</td>
            <td>@item.Country</td>
        </tr>
    }
    </tbody>
</table>
